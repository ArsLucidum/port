<svg width="800" height="600" viewBox="0 0 800 600" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="800" height="600" fill="#f9fafb"/>
  
  <!-- Testing Environment -->
  <rect x="50" y="50" width="700" height="400" rx="8" fill="#ffffff" stroke="#e5e7eb" stroke-width="2"/>
  
  <!-- Table -->
  <rect x="200" y="200" width="400" height="150" rx="8" fill="#f3f4f6"/>
  
  <!-- Tablet Device -->
  <rect x="280" y="220" width="240" height="180" rx="12" fill="#374151"/>
  <rect x="295" y="240" width="210" height="140" rx="6" fill="#ffffff"/>
  
  <!-- Tablet Screen Content -->
  <rect x="305" y="250" width="190" height="25" fill="#3b82f6"/>
  <rect x="315" y="257" width="80" height="3" fill="#dbeafe"/>
  <rect x="315" y="265" width="60" height="2" fill="#bfdbfe"/>
  
  <rect x="305" y="285" width="190" height="80" fill="#f8fafc"/>
  <rect x="315" y="295" width="170" height="6" rx="3" fill="#e2e8f0"/>
  <rect x="315" y="310" width="120" height="4" rx="2" fill="#f1f5f9"/>
  <rect x="315" y="325" width="140" height="4" rx="2" fill="#f1f5f9"/>
  <rect x="315" y="340" width="100" height="4" rx="2" fill="#f1f5f9"/>
  
  <!-- Person -->
  <circle cx="150" cy="280" r="25" fill="#fbbf24"/>
  <rect x="135" y="305" width="30" height="50" rx="15" fill="#3b82f6"/>
  <rect x="125" y="330" width="15" height="35" rx="7" fill="#1f2937"/>
  <rect x="160" y="330" width="15" height="35" rx="7" fill="#1f2937"/>
  <rect x="120" y="310" width="15" height="25" rx="7" fill="#f59e0b"/>
  <rect x="165" y="310" width="15" height="25" rx="7" fill="#f59e0b"/>
  
  <!-- Researcher -->
  <circle cx="650" cy="280" r="20" fill="#f3e8ff"/>
  <rect x="638" y="300" width="24" height="40" rx="12" fill="#8b5cf6"/>
  <rect x="630" y="320" width="12" height="28" rx="6" fill="#1f2937"/>
  <rect x="658" y="320" width="12" height="28" rx="6" fill="#1f2937"/>
  
  <!-- Notebook -->
  <rect x="620" y="260" width="20" height="25" fill="#ffffff" stroke="#d1d5db"/>
  <rect x="622" y="265" width="16" height="2" fill="#9ca3af"/>
  <rect x="622" y="270" width="12" height="1" fill="#d1d5db"/>
  <rect x="622" y="275" width="14" height="1" fill="#d1d5db"/>
  
  <!-- Speech Bubbles -->
  <rect x="60" y="150" width="120" height="40" rx="8" fill="#dbeafe"/>
  <text x="120" y="165" text-anchor="middle" fill="#1e40af" font-family="Arial, sans-serif" font-size="10">"I can actually find</text>
  <text x="120" y="180" text-anchor="middle" fill="#1e40af" font-family="Arial, sans-serif" font-size="10">my test results now!"</text>
  
  <rect x="600" y="150" width="120" height="40" rx="8" fill="#f3e8ff"/>
  <text x="660" y="165" text-anchor="middle" fill="#7c3aed" font-family="Arial, sans-serif" font-size="10">"Task completed</text>
  <text x="660" y="180" text-anchor="middle" fill="#7c3aed" font-family="Arial, sans-serif" font-size="10">successfully!"</text>
  
  <!-- Arrows -->
  <path d="M120 190 L140 220" stroke="#64748b" stroke-width="2" fill="none" marker-end="url(#arrowhead)"/>
  <path d="M660 190 L640 220" stroke="#64748b" stroke-width="2" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Title and Context -->
  <text x="400" y="30" text-anchor="middle" fill="#374151" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Healthcare Portal Usability Testing</text>
  
  <!-- Demographics Info -->
  <rect x="50" y="480" width="700" height="80" rx="6" fill="#f0f9ff"/>
  <text x="70" y="505" fill="#1e40af" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Testing Session Details:</text>
  <text x="70" y="525" fill="#1e40af" font-family="Arial, sans-serif" font-size="11">• 15 participants aged 35-78 years</text>
  <text x="70" y="540" fill="#1e40af" font-family="Arial, sans-serif" font-size="11">• Mix of tech-savvy and limited digital literacy users</text>
  
  <text x="400" y="525" fill="#1e40af" font-family="Arial, sans-serif" font-size="11">• Tasks: Find test results, schedule appointment, send message</text>
  <text x="400" y="540" fill="#1e40af" font-family="Arial, sans-serif" font-size="11">• Environment: Realistic healthcare setting with actual data</text>
  
  <text x="400" y="585" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="14">Usability Testing with Diverse Age Groups - Healthcare Portal Redesign</text>
  
  <!-- Arrow marker definition -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="0" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#64748b"/>
    </marker>
  </defs>
</svg>